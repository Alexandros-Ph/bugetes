{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "createdAt": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    },
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "productId"
    }
  },
  "acls": [
	  {
		  "accessType": "WRITE",
		  "principalType": "ROLE",
		  "principalId": "$everyone",
		  "permission": "DENY"
	  },
	  {
		  "accessType": "WRITE",
		  "principalType": "ROLE",
		  "principalId": "$authenticated",
		  "permission": "ALLOW"
	  }
  ],
  "methods": {}
}
