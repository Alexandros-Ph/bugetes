{
  "name": "Review",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "http": {"path": "/api/Reviews"},
  "properties": {
    "description": {
      "type": "string"
    },
    "stars": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "provider": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "provId"
    },
    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
