{
  "name": "ProviderPlus",
  "description": "Maps addresses to providers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "address": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "GeoPoint",
      "required": false
    },
    "code": {
      "type": "number",
      "required": false
    },
    "tags": {
      "type": "string",
      "required": false
    },
    "withdrawn": {
      "type": "Boolean",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
