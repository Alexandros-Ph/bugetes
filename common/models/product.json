{
  "name": "Product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "createdAt": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "number",
      "required": true
    },
    "expiredAfter": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "provider": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId"
    },
    "order": {
      "type": "hasMany",
      "model": "Order",
      "foreignKey": "productId"
    }
  },
  "acls": [],
  "methods": {}
}
